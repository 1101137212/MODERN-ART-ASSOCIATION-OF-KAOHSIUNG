<!DOCTYPE html>
<html lang="en">
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href='http://fonts.googleapis.com/css?family=Indie+Flower' rel='stylesheet' type='text/css'>
    <link href="~/Content/Dictionary/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Dictionary/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />
    <link href="~/Content/Dictionary/font-awesome.css" rel="stylesheet" />

    <script src="~/Scripts/Dictionary/jquery.min.js"></script>
    <script src="~/Scripts/Dictionary/bootstrap.min.js"></script>
    <script src="~/Scripts/scripts.js"></script>
    <script src="~/Scripts/main.js"></script>
    <script src="~/Scripts/Dictionary/jquery-1.10.2.min.js"></script>

    <script src="http://use.edgefonts.net/poiret-one.js"></script>

</head>
<body>
    <div>
        <div class="nulogo container"></div>
        <div class="container">
            <nav class="navbar navbar-default">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar" style=" padding-top: 20px; ">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse " sid="myNavbar">
                        <ul class="nav navbar-nav navbar-right dot">
                            <li class="nat"><a href="#">作品集</a></li>
                        </ul>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="nat"><a href="#">展覽快訊</a></li>
                        </ul>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="dropdown nat">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">最新消息<strong class="caret"></strong></a>
                                <ul class="dropdown-menu ">
                                    <li><a href="#">最新活動消息</a></li>
                                    <li><a href="#">得獎事蹟</a></li>
                                    <li><a href="#">媒體報導</a></li>
                                </ul>
                            </li>
                        </ul>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="dropdown nat">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">成員簡介<strong class="caret"></strong></a>
                                <ul class="dropdown-menu ">
                                    <li><a href="#">學會畫家</a></li>
                                </ul>
                            </li>
                        </ul>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="dropdown nat">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">關於學會<strong class="caret"></strong></a>
                                <ul class="dropdown-menu ">
                                    <li><a href="#">設立沿革</a></li>
                                    <li><a href="#">學會大事跡</a></li>
                                    <li><a href="#">學會組織</a></li>
                                    <li><a href="#">入會申請</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div><!-- nav -->
    </div>
    <div id="id_footer">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year MAAK</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
